<div class="row">
  <div class="large-8 columns">
    <h1>Boston Bars</h1>
    <ul>
      <% @bars.each do |bar| %>
        <li>  <%= bar.name %>  </li>
      <% end %>
    </ul>
  </div>

  <div class="large-4 columns">
    <%= form_tag bars_path, method: :get do %>
    <p>
      <%= text_field_tag :search, params[:search] %>
      <%= submit_tag "Search", name: nil %>
    </p>
    <% end %>

    <%= link_to "Add New Bar", new_bar_path %>
  </div>
</div>
